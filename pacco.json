{
  "browsers": ["ie >= 10", "ie_mob >= 10", "edge >= 13", "ff >= 30", "chrome >= 34", "safari >= 7", "opera >= 23", "ios >= 7", "android >= 4.4", "bb >= 10"],
  "components": {
    "_": true,
    "core": {
      "jquery/vendor": false,
      "lodash/vendor": false,
      "polyfills": false
    },
    "decorators": true,
    "lib": {
      "emoji/data/raw/make": false,
      "emoji/data/raw/raw": false
    },
    "widgets": {
      "boilerplate": false,
      "chart/vendor": false,
      "datatables/vendor": false,
      "editor/vendor": false
    }
  },
  "paths": {
    "tokens": {
      "bundle": "svelto"
    }
  },
  "plugins": {
    "substitute": {
      "options": {
        "substitutions": {
          "svelto": {
            "version": "1.2.48"
          }
        }
      }
    }
  },
  "environments": {
    "production": {
      "components": {
        "core": {
          "debug/debug.js": false
        }
      }
    },
    "development": {
      "components": {
        "core": {
          "regenerator_runtime.js": false
        }
      }
    }
  }
}
