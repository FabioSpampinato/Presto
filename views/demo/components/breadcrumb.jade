//- components/breadcrumb.jade

extends ../../layout.jade

append content
    +divider Base

    +container
        +breadcrumb
            a(href="#", title="Link 1") Link 1
            a(href="#", title="Link 2") Link 2
            a(href="#", title="Link 3") Link 3
            a(href="#", title="Link 4") Link 4

    +divider Slashed

    +container
        +breadcrumb-slashed
            a(href="#", title="Link 1") Link 1
            a(href="#", title="Link 2") Link 2
            a(href="#", title="Link 3") Link 3
            a(href="#", title="Link 4") Link 4
