
extends ../layout.jade

//- Browser

append main
  +divider Browser

  +multiple.fluid
    +container-simple.outlined.isMobile isMobile
    +container-simple.outlined.isTablet isTablet
    +container-simple.outlined.isAndroid isAndroid
    +container-simple.outlined.isIOS isIOS
    +container-simple.outlined.isMac isMac
    +container-simple.outlined.isIE isIE

append body-end
  +script.
    for ( var prop in $.browser ) {
      $('.' + prop).toggleClass ( 'green', $.browser[prop] );
    }
